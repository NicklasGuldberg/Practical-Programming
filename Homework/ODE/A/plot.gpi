set terminal png
set output 'plot1.png'
set key top right
set xlabel '$x$'
set ylabel '$y$'
plot 'out.txt' index 0 using 1:2 with lines title "y[0]"

set output 'plot2.png'
set key bottom right
set xlabel '$t$'
set grid
set xticks 0,2,10
set yticks -4,2,2
plot 'out.txt' index 1 using 1:2 with lines color blue title "theta(t)"
replot 'out.txt' index 1 using 1:3 with lines color green title "omega(t)"