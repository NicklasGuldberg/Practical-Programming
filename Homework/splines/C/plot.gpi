set terminal png
set output 'plot.png'
set key below

plot 'out.txt' index 0 using 1:2 with points title "Points"
replot 'out.txt' index 1 using 1:2 with lines title "Cubic Spline $s_i(x)$"
replot 'out.txt' index 2 using 1:2 with lines title "Derivative of Spline $s_i'(x)$"
replot 'out.txt' index 3 using 1:2 with lines title "Integral of Spline $\int_0^x s_i(x') dx'$"
interpolate spline f() 'out.txt' index 0 using 1:2
replot f(x) with lines linetype 2 title "Pyxplot Cubic Spline"